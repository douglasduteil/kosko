{
  "name": "@kosko/yaml",
  "version": "1.0.2",
  "description": "Load Kubernetes YAML files in kosko.",
  "homepage": "https://kosko.dev",
  "repository": "https://github.com/tommy351/kosko/tree/master/packages/yaml",
  "author": "Tommy Chen <tommy351@gmail.com>",
  "license": "MIT",
  "main": "dist/index.node.js",
  "module": "dist/index.node.mjs",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "sideEffects": false,
  "exports": {
    "node": {
      "import": "./dist/index.node.mjs",
      "require": "./dist/index.node.js"
    },
    "default": "./dist/index.mjs"
  },
  "files": [
    "/dist/",
    "/deno_dist/",
    "/mod.ts"
  ],
  "keywords": [
    "kubernetes",
    "kosko",
    "yaml"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@kosko/require": "workspace:*",
    "@kosko/system-utils": "workspace:*",
    "debug": "^4.3.1",
    "isomorphic-fetch": "^3.0.0",
    "js-yaml": "^4.0.0",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/js-yaml": "^4.0.0",
    "fetch-mock": "^9.11.0",
    "kubernetes-models": "^1.5.2",
    "temp-dir": "^2.0.0",
    "tmp-promise": "^3.0.2"
  },
  "peerDependencies": {
    "kubernetes-models": ">=1.0.3"
  }
}
